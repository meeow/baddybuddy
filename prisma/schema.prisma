// Prisma docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
    previewFeatures = ["jsonProtocol"]
}

datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
    relationMode = "prisma"
}

model Group {
    id        String   @id
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    signupState Json?
}
